<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
                           http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-3.2.xsd">

    <import resource="ConnectorTestCaseSpringBeans.xml"/>

    <util:map id="retrieveTestData"
              key-type="java.lang.String" map-class="java.util.HashMap" scope="prototype"
              value-type="java.lang.Object">
        <entry key="key" value="mykey"/>
        <entry key="key2" value="mykey2"/>
        <entry key="value" value="mulesoft"/>
        <entry key="default" value="muleion"/>
        <entry key="propertyName" value="testProperty"/>
    </util:map>

</beans>
